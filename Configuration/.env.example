# Dialpad API Configuration
# Copy this file to .env and fill in your actual values

# Your Dialpad API Bearer Token
DIALPAD_BEARER_TOKEN=your_actual_bearer_token_here

# Dialpad API Base URL (use sandbox for testing, production for live)
DIALPAD_API_BASE_URL=https://sandbox.dialpad.com/api/v2
# DIALPAD_API_BASE_URL=https://dialpad.com/api/v2  # Production environment

# Optional: Company ID (if you need to filter by specific company)
# COMPANY_ID=your_company_id_here

# Request timeout in seconds
REQUEST_TIMEOUT=30

# Enable debug logging
DEBUG=false